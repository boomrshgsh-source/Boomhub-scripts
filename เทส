-- =========================
-- Final Initialization
-- =========================
-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏•‡∏∏‡πà‡∏° radio button
if TargetLockType == "Head" then
    headLockToggle:Set(true)
    bodyLockToggle:Set(false)
else
    headLockToggle:Set(false)
    bodyLockToggle:Set(true)
end

-- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å callback functions ‡πÑ‡∏õ‡∏¢‡∏±‡∏á config
myConfig:Register("SilentAimEnabled", function(v) 
    SilentAimEnabled = v 
    silentAimToggle:Set(v)
end)

myConfig:Register("GoodGunEnabled", function(v) 
    GoodGunEnabled = v 
    goodGunToggle:Set(v)
end)

myConfig:Register("TargetLockType", function(v) 
    TargetLockType = v 
    if v == "Head" then
        headLockToggle:Set(true)
        bodyLockToggle:Set(false)
    else
        headLockToggle:Set(false)
        bodyLockToggle:Set(true)
    end
end)

myConfig:Register("FOVRadius", function(v) 
    FOVRadius = v 
end)

-- Load saved configuration
task.wait(1)
myConfig:Load()

-- Print initialization message
print("=== Advanced Silent Aim System ===")
print("‚úÖ System Initialized Successfully!")
print("Target Lock Type: " .. TargetLockType)
print("FOV Radius: " .. FOVRadius)
print("Press 'G' to open/close UI")
print("================================")

-- Mobile notification
if isMobile then
    print("üì± Mobile Mode: UI optimized for touch controls")
end
