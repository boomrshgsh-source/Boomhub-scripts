        infoText.Visible = v and (SilentAimEnabled or GoodGunEnabled)
        targetTypeText.Visible = v and (SilentAimEnabled or GoodGunEnabled)
        print("Info Text: " .. (v and "SHOWN" or "HIDDEN"))
    end
})

local showFOVToggle = VisualTab:Toggle({
    Title = "Show FOV Circle",
    Desc = "‡πÅ‡∏™‡∏î‡∏á‡∏ß‡∏á‡∏Å‡∏•‡∏°‡πÄ‡∏Ç‡∏ï‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö",
    Value = true,
    Callback = function(v)
        if SilentFOVCircle then
            SilentFOVCircle.Visible = v and (SilentAimEnabled or GoodGunEnabled)
        end
        print("FOV Circle: " .. (v and "SHOWN" or "HIDDEN"))
    end
})

VisualTab:Button({
    Title = "Toggle All Visuals",
    Desc = "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏†‡∏≤‡∏û‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î",
    Callback = function()
        local currentState = showTracerToggle.Value
        local newState = not currentState
        
        showTracerToggle:Set(newState)
        showCrosshairToggle:Set(newState)
        showInfoToggle:Set(newState)
        showFOVToggle:Set(newState)
        
        print("All visuals: " .. (newState and "ENABLED" or "DISABLED"))
    end
})

-- =========================
-- Weapons Tab (‡πÅ‡∏ö‡∏ö‡∏á‡πà‡∏≤‡∏¢)
-- =========================
local WeaponsTab = Window:Tab({Title = "Weapons", Icon = "gun"})

WeaponsTab:Section({Title = "Weapon Management"})

local weaponSelection = {}
local function updateWeaponSelection()
    local selectedCount = 0
    for _, enabled in pairs(weaponSelection) do
        if enabled then selectedCount = selectedCount + 1 end
    end
    
    WeaponsTab:Section({Title = string.format("Selected: %d/%d Weapons", selectedCount, #GunNames)})
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏ö‡∏≤‡∏á‡∏™‡πà‡∏ß‡∏ô
local popularWeapons = {
    "AK47", "M4A1", "AWM", "Desert Eagle", "MP5", "Shotgun", "Vector"
}

for _, weaponName in ipairs(popularWeapons) do
    WeaponsTab:Toggle({
        Title = weaponName,
        Desc = "‡∏≠‡∏ô‡∏∏‡∏ç‡∏≤‡∏ï Silent Aim ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö " .. weaponName,
        Value = true,
        Callback = function(v)
            weaponSelection[weaponName] = v
            GunLookup[weaponName:lower()] = v
            updateWeaponSelection()
            print(weaponName .. ": " .. (v and "ENABLED" or "DISABLED"))
        end
    })
end

WeaponsTab:Button({
    Title = "Enable Popular Weapons",
    Desc = "‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏¢‡∏≠‡∏î‡∏ô‡∏¥‡∏¢‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î",
    Callback = function()
        for _, weaponName in ipairs(popularWeapons) do
            weaponSelection[weaponName] = true
            GunLookup[weaponName:lower()] = true
        end
        updateWeaponSelection()
        print("‚úÖ All popular weapons enabled")
    end
})

