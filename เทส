    end
})

local showCrosshairToggle = VisualTab:Toggle({
    Title = "Show Crosshair",
    Desc = "‡πÅ‡∏™‡∏î‡∏á‡∏à‡∏∏‡∏î‡πÄ‡∏•‡πá‡∏á‡∏ö‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢",
