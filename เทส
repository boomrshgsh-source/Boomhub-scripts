local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local Debris = game:GetService("Debris")
local Workspace = game:GetService("Workspace")

-- =========================
-- Local Player & Character
-- =========================
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local Humanoid = Character:WaitForChild("Humanoid")
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- =========================
-- Camera & Mouse
-- =========================
local Camera = Workspace.CurrentCamera
local Mouse = LocalPlayer:GetMouse()
local isMobile = UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled

-- =========================
-- UI Setup
-- =========================
local Window = WindUI:CreateWindow({
    Title = "Advanced Silent Aim - Target Select",
    Icon = "rbxassetid://87372447687978",
    Author = "Enhanced System",
    Folder = "SilentAim Hub",
    Size = UDim2.fromOffset(500, 600),
    Theme = "Dark",
    Transparent = true,
    Resizable = false,
    KeyCode = Enum.KeyCode.G
})

-- =========================
-- ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÅ‡∏•‡∏∞‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤
-- =========================
local SilentAimEnabled = false
local GoodGunEnabled = false
local FOVRadius = 120
local CurrentTarget = nil
local TargetLockType = "Head" -- "Head" ‡∏´‡∏£‡∏∑‡∏≠ "Humanoid"
local SilentFOVCircle = nil
-- =========================
-- ‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏ó‡∏µ‡πà‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö
-- =========================
local GunNames = {
    "P226", "MP5", "M24", "Draco", "Glock", "Sawnoff", "Uzi", "G3", "C9",
    "Hunting Rifle", "Anaconda", "AK47", "Remington", "Double Barrel",
    "AR-15", "M4A1", "Desert Eagle", "Vector", "AWM", "Shotgun", "Rifle"
}

-- ‡πÉ‡∏ä‡πâ Set ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏ó‡∏µ‡πà‡πÄ‡∏£‡πá‡∏ß‡∏Ç‡∏∂‡πâ‡∏ô
local GunLookup = {}
for _, name in ipairs(GunNames) do
    GunLookup[name:lower()] = true
end

-- =========================
-- Advanced Prediction System
-- =========================
local AutoPrediction = {
    prevPrediction = 0.085,
    prevHorizontalVelocity = Vector3.new(0, 0, 0),
    
    -- ‡πÄ‡∏≠‡∏≤‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏ô‡∏ß‡∏ô‡∏≠‡∏ô (‡∏ï‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏´‡∏°‡∏∏‡∏ô‡∏≠‡∏≠‡∏Å)
    GetHorizontalVelocity = function(self, velocity)
        return Vector3.new(velocity.X, 0, velocity.Z)
    end,
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏à‡∏£‡∏¥‡∏á (‡πÑ‡∏°‡πà‡∏ô‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏´‡∏°‡∏∏‡∏ô)
    IsActuallyMoving = function(self, humanoidRootPart)
        if not humanoidRootPart then return false end
        
        local velocity = humanoidRootPart.Velocity
        local horizontalVelocity = self:GetHorizontalVelocity(velocity)
        
        -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ Humanoid ‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö MoveDirection
        local character = humanoidRootPart.Parent
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
        
        if humanoid then
            local moveDirection = humanoid.MoveDirection
            local isMoving = moveDirection.Magnitude > 0.1
            local horizontalSpeed = horizontalVelocity.Magnitude
            
            return isMoving and horizontalSpeed > 2
        end
        
        return horizontalVelocity.Magnitude > 2
    end,
    
    Calculate = function(self, targetPosition, targetVelocity, distance, isActuallyMoving)
        local basePred = 0.085
        local maxSpeed = 25
        local maxDist = 350
        local learnRate = 0.25
        local smoothRate = 0.5

        -- ‡πÉ‡∏ä‡πâ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏ô‡∏ß‡∏ô‡∏≠‡∏ô
        local horizontalVelocity = self:GetHorizontalVelocity(targetVelocity)
        local speed = horizontalVelocity.Magnitude
        
        -- ‡∏õ‡∏£‡∏±‡∏ö‡∏ï‡∏≤‡∏°‡∏ß‡πà‡∏≤‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏à‡∏£‡∏¥‡∏á‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        local speedFactor = 0
        if isActuallyMoving then
            speedFactor = math.clamp(speed / maxSpeed, 0, 1)
        else
            speedFactor = math.clamp(speed / 5, 0, 0.2)
        end

        local distFactor = math.clamp(distance / maxDist, 0, 1)

        -- ACCELERATION FACTOR (‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÅ‡∏ô‡∏ß‡∏ô‡∏≠‡∏ô)
        local accel = (horizontalVelocity - self.prevHorizontalVelocity).Magnitude
        local accelFactor = math.clamp(accel / 15, 0, 1)
        self.prevHorizontalVelocity = horizontalVelocity

        -- FINAL PREDICTION CALCULATION
        local prediction = basePred
        
        if isActuallyMoving and speed > 3 then
            prediction = prediction + speedFactor * 0.045
        end
        
        prediction = prediction + distFactor * 0.025
        prediction = prediction + accelFactor * 0.015

        -- ADAPTIVE LEARNING
        prediction = self.prevPrediction + (prediction - self.prevPrediction) * learnRate

        -- SMOOTHING
        prediction = self.prevPrediction + (prediction - self.prevPrediction) * smoothRate
        self.prevPrediction = prediction

        return math.clamp(prediction, 0.05, 0.15)
    end,
    
    GetPredictedPosition = function(self, pos, vel, distance, isActuallyMoving)
        local pred = self:Calculate(pos, vel, distance, isActuallyMoving)
        return pos + vel * pred, pred
    end
}

-- =========================
-- Cache ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∞‡∏™‡∏¥‡∏ó‡∏ò‡∏¥‡∏†‡∏≤‡∏û
-- =========================
local cachedTarget = nil
local lastTargetUpdate = 0
local cachedPlayers = {}
-- =========================
-- Drawing Objects ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Visuals
-- =========================
local tracerLine = Drawing.new("Line")
tracerLine.Color = Color3.fromRGB(255, 50, 50)
tracerLine.Thickness = 2
tracerLine.Visible = false
tracerLine.ZIndex = 1

local crosshair = Drawing.new("Circle")
crosshair.Color = Color3.fromRGB(255, 0, 0)
crosshair.Thickness = 2
crosshair.Radius = 6
crosshair.Filled = false
crosshair.Visible = false
crosshair.ZIndex = 2

local infoText = Drawing.new("Text")
infoText.Color = Color3.fromRGB(255, 255, 255)
infoText.Size = 13
infoText.Outline = true
infoText.Visible = false
infoText.ZIndex = 3

local targetTypeText = Drawing.new("Text")
targetTypeText.Color = Color3.fromRGB(0, 255, 255)
targetTypeText.Size = 15
targetTypeText.Outline = true
targetTypeText.Visible = false
targetTypeText.ZIndex = 3

-- =========================
-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ä‡πà‡∏ß‡∏¢‡πÄ‡∏´‡∏•‡∏∑‡∏≠
-- =========================
local function IsValidTarget(player)
    if player == LocalPlayer then return false end
    if not player.Character then return false end
    
    local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
    if not humanoid or humanoid.Health <= 0 then return false end
    
    if TargetLockType == "Head" then
        local head = player.Character:FindFirstChild("Head")
        if not head then return false end
    else -- Humanoid
        local hrp = player.Character:FindFirstChild("HumanoidRootPart")
        if not hrp then return false end
    end
    
    return true
end

local function UpdatePlayerCache()
    cachedPlayers = {}
    for _, player in pairs(Players:GetPlayers()) do
        if IsValidTarget(player) then
            table.insert(cachedPlayers, player)
        end
    end
    return cachedPlayers
end

-- ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏î‡πâ‡∏ß‡∏¢ FOV
local function GetClosestTarget(forceUpdate)
    local currentTime = tick()
    
    if forceUpdate or currentTime - lastTargetUpdate > 0.05 then
        UpdatePlayerCache()
        lastTargetUpdate = currentTime
        cachedTarget = nil
    end
    
    if cachedTarget and IsValidTarget(cachedTarget) then
        return cachedTarget
    end
    
    local closest = nil
    local shortestDistance = FOVRadius
    local center = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
    
    for _, player in ipairs(cachedPlayers) do
        if player ~= LocalPlayer and player.Character then
            local targetPart = nil
            
            if TargetLockType == "Head" then
                targetPart = player.Character:FindFirstChild("Head")
            else
                targetPart = player.Character:FindFirstChild("HumanoidRootPart")
            end
            
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            
            if targetPart and humanoid and humanoid.Health > 0 then
                local screenPos, onScreen = Camera:WorldToViewportPoint(targetPart.Position)
                if onScreen then
                    local screenVector = Vector2.new(screenPos.X, screenPos.Y)
                    local distanceFromCenter = (screenVector - center).Magnitude
                    
                    if distanceFromCenter <= FOVRadius then
                        if distanceFromCenter < shortestDistance then
                            shortestDistance = distanceFromCenter
                            closest = player
                        end
                    end
                end
            end
        end
    end
    
    cachedTarget = closest
    return closest
end

-- ‡∏£‡∏±‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≤‡∏°‡∏õ‡∏£‡∏∞‡πÄ‡∏†‡∏ó
local function GetTargetLockPosition(targetCharacter)
    if not targetCharacter then return nil end
    
    if TargetLockType == "Head" then
        local head = targetCharacter:FindFirstChild("Head")
        if head then
            return head.Position, head
        end
    else -- Humanoid
        local hrp = targetCharacter:FindFirstChild("HumanoidRootPart")
        if hrp then
            -- ‡πÄ‡∏•‡πá‡∏á‡∏ó‡∏µ‡πà‡∏Å‡∏•‡∏≤‡∏á‡∏ï‡∏±‡∏ß (HRP + offset)
            local humanoid = targetCharacter:FindFirstChildOfClass("Humanoid")
            local offset = Vector3.new(0, 1.5, 0) -- ‡∏õ‡∏£‡∏∞‡∏°‡∏≤‡∏ì‡∏Å‡∏•‡∏≤‡∏á‡∏ï‡∏±‡∏ß
            return hrp.Position + offset, hrp
        end
    end
    
    return nil
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò
local function IsHoldingAllowedGun()
    if not LocalPlayer.Character then return false end
    
    for _, child in pairs(LocalPlayer.Character:GetChildren()) do
        if (child:IsA("Tool") or child:IsA("Model")) and GunLookup[child.Name:lower()] then
            return true
        end
    end
    
    return false
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏≠‡∏¢‡∏π‡πà‡∏´‡∏•‡∏±‡∏á‡∏Å‡∏≥‡πÅ‡∏û‡∏á‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
local function isBehindWall(startPos, endPos)
    if not startPos or not endPos then
        return false
    end
    local ray = Ray.new(startPos, (endPos - startPos).Unit * (endPos - startPos).Magnitude)
    local ignoreList = {LocalPlayer.Character}
    if CurrentTarget and CurrentTarget.Character then
        table.insert(ignoreList, CurrentTarget.Character)
    end
    local hit, position = Workspace:FindPartOnRayWithIgnoreList(ray, ignoreList)
    return hit ~= nil
end
-- =========================
-- Hook FireServer
-- =========================
local send = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Send")
local oldFire

oldFire = hookfunction(send.FireServer, function(self, ...)
    local args = {...}
    
    if (SilentAimEnabled or GoodGunEnabled) and IsHoldingAllowedGun() then
        local target = GetClosestTarget()
        if target and target.Character then
            local lockPosition, lockPart = GetTargetLockPosition(target.Character)
            local humanoidRootPart = target.Character:FindFirstChild("HumanoidRootPart")
            local humanoid = target.Character:FindFirstChildOfClass("Humanoid")
            
            if lockPosition and humanoidRootPart and humanoid and humanoid.Health > 0 then
                local distance = (lockPosition - Camera.CFrame.Position).Magnitude
                local isActuallyMoving = AutoPrediction:IsActuallyMoving(humanoidRootPart)
                local aimPos, predictionUsed = AutoPrediction:GetPredictedPosition(
                    lockPosition,
                    humanoidRootPart.Velocity,
                    distance,
                    isActuallyMoving
                )
                
                -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏≠‡∏¢‡∏π‡πà‡∏´‡∏•‡∏±‡∏á‡∏Å‡∏≥‡πÅ‡∏û‡∏á‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
                local myHead = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Head")
                local isTargetBehindWall = false
                
                if myHead then
                    isTargetBehindWall = isBehindWall(myHead.Position, aimPos)
                end
                
                -- ‡∏£‡∏∞‡∏ö‡∏ö‡∏â‡∏•‡∏≤‡∏î: ‡∏ñ‡πâ‡∏≤‡∏≠‡∏¢‡∏π‡πà‡∏´‡∏•‡∏±‡∏á‡∏Å‡∏≥‡πÅ‡∏û‡∏á‡πÅ‡∏•‡∏∞‡πÄ‡∏õ‡∏¥‡∏î GoodGun ‡πÉ‡∏´‡πâ‡∏¢‡∏¥‡∏á‡∏ó‡∏∞‡∏•‡∏∏
                if GoodGunEnabled and isTargetBehindWall then
                    -- ‡πÇ‡∏´‡∏°‡∏î‡∏¢‡∏¥‡∏á‡∏ó‡∏∞‡∏•‡∏∏‡∏Å‡∏≥‡πÅ‡∏û‡∏á
                    args[4] = CFrame.new(math.huge, math.huge, math.huge)
                    args[5] = {
                        [1] = {
                            [1] = {
                                ["Instance"] = lockPart,
                                ["Normal"] = Vector3.new(0, 1, 0),
                                ["Position"] = aimPos
                            }
                        }
                    }
                    
                    -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏Å‡∏£‡∏∞‡∏™‡∏∏‡∏ô‡∏ó‡∏∞‡∏•‡∏∏‡∏Å‡∏≥‡πÅ‡∏û‡∏á
                    spawn(function()
                        if myHead then
                            local part = Instance.new("Part")
                            part.Anchored = true
                            part.CanCollide = false
                            part.Size = Vector3.new(0.15, 0.15, (aimPos - myHead.Position).Magnitude)
                            part.CFrame = CFrame.new(myHead.Position, aimPos) * CFrame.new(0, 0, -part.Size.Z / 2)
                            part.Material = Enum.Material.Neon
                            part.Transparency = 0.25
                            part.Color = Color3.fromRGB(0, 255, 0) -- ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß = ‡∏ó‡∏∞‡∏•‡∏∏‡∏Å‡∏≥‡πÅ‡∏û‡∏á
                            part.Parent = Workspace
                            Debris:AddItem(part, 4)
                        end
                    end)
                else
                    -- ‡πÇ‡∏´‡∏°‡∏î‡∏¢‡∏¥‡∏á‡∏õ‡∏Å‡∏ï‡∏¥
                    args[4] = CFrame.new(LocalPlayer.Character.Head.Position, aimPos)
                    args[5] = {
                        [1] = {
                            [1] = {
                                ["Instance"] = lockPart,
                                ["Normal"] = Vector3.new(0, 1, 0),
                                ["Position"] = aimPos
                            }
                        }
                    }
                    
                    -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏Å‡∏£‡∏∞‡∏™‡∏∏‡∏ô‡∏õ‡∏Å‡∏ï‡∏¥
                    spawn(function()
                        if myHead then
                            local part = Instance.new("Part")
                            part.Anchored = true
                            part.CanCollide = false
                            part.Size = Vector3.new(0.15, 0.15, (aimPos - myHead.Position).Magnitude)
                            part.CFrame = CFrame.new(myHead.Position, aimPos) * CFrame.new(0, 0, -part.Size.Z / 2)
                            part.Material = Enum.Material.Neon
                            part.Transparency = 0.25
                            part.Color = Color3.fromRGB(255, 50, 50) -- ‡πÅ‡∏î‡∏á = ‡∏õ‡∏Å‡∏ï‡∏¥
                            part.Parent = Workspace
                            Debris:AddItem(part, 4)
                        end
                    end)
                end
                
                -- ‡πÅ‡∏™‡∏î‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏Å‡∏≤‡∏£‡∏¢‡∏¥‡∏á
                print(string.format("[Silent Aim] Target: %s | Type: %s | Distance: %.1f | Prediction: %.3f | Wallbang: %s", 
                    target.Name, TargetLockType, distance, predictionUsed, 
                    (GoodGunEnabled and isTargetBehindWall) and "YES" or "NO"))
            end
        end
    end
    
    return oldFire(self, unpack(args))
end)
-- =========================
-- Update Visuals
-- =========================
RunService.RenderStepped:Connect(function()
    if not SilentAimEnabled and not GoodGunEnabled then
        tracerLine.Visible = false
        crosshair.Visible = false
        infoText.Visible = false
        targetTypeText.Visible = false
        return
    end
    
    local target = GetClosestTarget()
    CurrentTarget = target
    
    if target and target.Character then
        local lockPosition, lockPart = GetTargetLockPosition(target.Character)
        local humanoidRootPart = target.Character:FindFirstChild("HumanoidRootPart")
        
        if lockPosition and humanoidRootPart then
            local distance = (lockPosition - Camera.CFrame.Position).Magnitude
            local isActuallyMoving = AutoPrediction:IsActuallyMoving(humanoidRootPart)
            local predictedPos, predictionUsed = AutoPrediction:GetPredictedPosition(
                lockPosition,
                humanoidRootPart.Velocity,
                distance,
                isActuallyMoving
            )
            
            local screenPos, onScreen = Camera:WorldToViewportPoint(predictedPos)
            local horizontalVelocity = AutoPrediction:GetHorizontalVelocity(humanoidRootPart.Velocity)
            local targetSpeed = horizontalVelocity.Magnitude
            
            if onScreen then
                -- Update Tracer Line
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Head") then
                    local ourHead = LocalPlayer.Character.Head
                    local screenStart = Camera:WorldToViewportPoint(ourHead.Position)
                    tracerLine.From = Vector2.new(screenStart.X, screenStart.Y)
                    tracerLine.To = Vector2.new(screenPos.X, screenPos.Y)
                    
                    -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏ï‡∏≤‡∏°‡πÇ‡∏´‡∏°‡∏î
                    if GoodGunEnabled then
                        local isTargetBehindWall = isBehindWall(ourHead.Position, predictedPos)
                        tracerLine.Color = isTargetBehindWall and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 50, 50)
                    else
                        tracerLine.Color = Color3.fromRGB(255, 50, 50)
                    end
                    
                    tracerLine.Visible = true
                end
                
                -- Update Crosshair
                crosshair.Position = Vector2.new(screenPos.X, screenPos.Y)
                
                -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ Crosshair ‡∏ï‡∏≤‡∏°‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
                if isActuallyMoving then
                    crosshair.Color = Color3.fromRGB(0, 255, 0)  -- ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß = ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
                else
                    crosshair.Color = Color3.fromRGB(255, 50, 50) -- ‡πÅ‡∏î‡∏á = ‡πÑ‡∏°‡πà‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
                end
                crosshair.Visible = true
                
                -- Update Info Text
                local movingStatus = isActuallyMoving and "MOVING" or "STATIONARY"
                local wallbangStatus = ""
                
                if GoodGunEnabled then
                    local isTargetBehindWall = isBehindWall(LocalPlayer.Character.Head.Position, predictedPos)
                    wallbangStatus = isTargetBehindWall and " | WALLBANG" or ""
                end
                
                infoText.Text = string.format("%s\nDist: %.1f | Speed: %.1f\nPred: %.3f | %s%s", 
                    target.Name, distance, targetSpeed, predictionUsed, movingStatus, wallbangStatus)
                infoText.Position = Vector2.new(screenPos.X + 15, screenPos.Y + 15)
                infoText.Visible = true
                
                -- Update Target Type Text
                targetTypeText.Text = string.format("Lock: %s", TargetLockType)
                targetTypeText.Position = Vector2.new(screenPos.X + 15, screenPos.Y + 50)
                targetTypeText.Color = (TargetLockType == "Head") and Color3.fromRGB(255, 100, 100) or Color3.fromRGB(100, 100, 255)
                targetTypeText.Visible = true
            else
                tracerLine.Visible = false
                crosshair.Visible = false
                infoText.Visible = false
                targetTypeText.Visible = false
            end
        else
            tracerLine.Visible = false
            crosshair.Visible = false
            infoText.Visible = false
            targetTypeText.Visible = false
        end
    else
        tracerLine.Visible = false
        crosshair.Visible = false
        infoText.Visible = false
        targetTypeText.Visible = false
    end
end)

-- =========================
-- ‡∏™‡∏£‡πâ‡∏≤‡∏á FOV Circle (Desktop/Mobile)
-- =========================
if not isMobile then
    SilentFOVCircle = Drawing.new("Circle")
    SilentFOVCircle.Color = Color3.fromRGB(255, 255, 255)
    SilentFOVCircle.Thickness = 2
    SilentFOVCircle.NumSides = 64
    SilentFOVCircle.Filled = false
    SilentFOVCircle.Transparency = 0.8
    SilentFOVCircle.Visible = false
else
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "MobileFOV"
    ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
    
    SilentFOVCircle = Instance.new("Frame")
    SilentFOVCircle.Size = UDim2.fromOffset(FOVRadius * 2, FOVRadius * 2)
    SilentFOVCircle.AnchorPoint = Vector2.new(0.5, 0.5)
    SilentFOVCircle.Position = UDim2.fromScale(0.5, 0.5)
    SilentFOVCircle.BackgroundTransparency = 1
    
    local circleUI = Instance.new("UICorner")
    circleUI.CornerRadius = UDim.new(1, 0)
    circleUI.Parent = SilentFOVCircle
    
    local border = Instance.new("UIStroke")
    border.Color = Color3.fromRGB(255, 255, 255)
    border.Thickness = 2
    border.Transparency = 0.2
    border.Parent = SilentFOVCircle
    SilentFOVCircle.Parent = ScreenGui
end

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï FOV Circle
RunService.RenderStepped:Connect(function()
    if SilentFOVCircle then
        SilentFOVCircle.Visible = SilentAimEnabled or GoodGunEnabled
        if isMobile then
            SilentFOVCircle.Position = UDim2.fromScale(0.5, 0.5)
            SilentFOVCircle.Size = UDim2.fromOffset(FOVRadius * 2, FOVRadius * 2)
        else
            SilentFOVCircle.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
            SilentFOVCircle.Radius = FOVRadius
        end
    end
end)
-- =========================
-- Config Management
-- =========================
local ConfigManager = Window.ConfigManager
local myConfig = ConfigManager:CreateConfig("EnhancedSilentAim")

-- =========================
-- Main Tab
-- =========================
local MainTab = Window:Tab({Title = "Main", Icon = "crosshair"})

-- Section 1: ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏´‡∏•‡∏±‡∏Å
MainTab:Section({Title = "Activation"})

local silentAimToggle = MainTab:Toggle({
    Title = "Enable Silent Aim",
    Desc = "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Silent Aim ‡∏û‡∏∑‡πâ‡∏ô‡∏ê‡∏≤‡∏ô",
    Value = false,
    Callback = function(v)
        SilentAimEnabled = v
        tracerLine.Visible = v or GoodGunEnabled
        crosshair.Visible = v or GoodGunEnabled
        infoText.Visible = v or GoodGunEnabled
        targetTypeText.Visible = v or GoodGunEnabled
        if SilentFOVCircle then
            SilentFOVCircle.Visible = v or GoodGunEnabled
        end
        
        if v and GoodGunEnabled then
            GoodGunEnabled = false
        end
        print("Silent Aim: " .. (v and "ENABLED" or "DISABLED"))
    end
})

local goodGunToggle = MainTab:Toggle({
    Title = "Enable Wallbang",
    Desc = "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡∏¢‡∏¥‡∏á‡∏ó‡∏∞‡∏•‡∏∏‡∏Å‡∏≥‡πÅ‡∏û‡∏á (GoodGun)",
    Value = false,
    Callback = function(v)
        GoodGunEnabled = v
        tracerLine.Visible = v or SilentAimEnabled
        crosshair.Visible = v or SilentAimEnabled
        infoText.Visible = v or SilentAimEnabled
        targetTypeText.Visible = v or SilentAimEnabled
        if SilentFOVCircle then
            SilentFOVCircle.Visible = v or SilentAimEnabled
        end
        
        if v and SilentAimEnabled then
            SilentAimEnabled = false
            silentAimToggle:Set(false)
        end
        print("Wallbang Mode: " .. (v and "ENABLED" or "DISABLED"))
    end
})

-- Section 2: Target Selection
MainTab:Section({Title = "Target Lock Type"})

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á radio button group
local targetTypeGroup = {}

local headLockToggle = MainTab:Toggle({
    Title = "Lock to Head",
    Desc = "‡∏•‡πá‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡∏®‡∏µ‡∏£‡∏©‡∏∞ (‡πÅ‡∏°‡πà‡∏ô‡∏¢‡∏≥‡∏™‡∏π‡∏á)",
    Value = TargetLockType == "Head",
    Callback = function(v)
        if v then
            TargetLockType = "Head"
            if targetTypeGroup["Body"] then
                targetTypeGroup["Body"]:Set(false)
            end
            print("Target lock set to: HEAD")
        end
    end
})
targetTypeGroup["Head"] = headLockToggle

local bodyLockToggle = MainTab:Toggle({
    Title = "Lock to Body",
    Desc = "‡∏•‡πá‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡∏•‡∏≥‡∏ï‡∏±‡∏ß (‡πÄ‡∏™‡∏ñ‡∏µ‡∏¢‡∏£‡∏Å‡∏ß‡πà‡∏≤)",
    Value = TargetLockType == "Humanoid",
    Callback = function(v)
        if v then
            TargetLockType = "Humanoid"
            if targetTypeGroup["Head"] then
                targetTypeGroup["Head"]:Set(false)
            end
            print("Target lock set to: BODY")
        end
    end
})
targetTypeGroup["Body"] = bodyLockToggle

-- Section 3: FOV Settings
MainTab:Section({Title = "Aiming Settings"})

MainTab:Slider({
    Title = "FOV Radius",
    Desc = "‡∏õ‡∏£‡∏±‡∏ö‡∏Ç‡∏ô‡∏≤‡∏î‡πÄ‡∏Ç‡∏ï‡∏Å‡∏≤‡∏£‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢",
    Step = 5,
    Value = {
        Min = 50,
        Max = 500,
        Default = FOVRadius
    },
    Callback = function(value)
        FOVRadius = tonumber(value) or 120
        if SilentFOVCircle then
            if isMobile then
                SilentFOVCircle.Size = UDim2.fromOffset(FOVRadius * 2, FOVRadius * 2)
            else
                SilentFOVCircle.Radius = FOVRadius
            end
        end
        print("FOV Radius set to: " .. FOVRadius)
    end
})
