--// üîë BOOM HUB Key System + Main UI
--// ‡∏Ñ‡∏µ‡∏¢‡πå: BOOM-HUBV2
local discordLink = "https://discord.gg/w9qmXKV3d"
local realKey = "BOOM-HUBV2"

-- üåü ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏Å
local function RunMainScript()
    -- ‡πÇ‡∏´‡∏•‡∏î UI Library
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/x2zu/OPEN-SOURCE-UI-ROBLOX/refs/heads/main/X2ZU%20UI%20ROBLOX%20OPEN%20SOURCE/DummyUi-leak-by-x2zu/fetching-main/Tools/Framework.luau"))()
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡∏´‡∏•‡∏±‡∏Å
    local Window = Library:Window({
        Title = "BOOM HUB",
        Desc = "by Boom",
        Icon = 105059922903197,
        Theme = "Dark",
        Config = { Keybind = Enum.KeyCode.LeftControl, Size = UDim2.new(0, 500, 0, 400) },
        CloseUIButton = { Enabled = true, Text = "BOOM" }
    })

    local ESPEnabled = false
    local AutoLockEnabled = false
    local AutoSwapEnabled = false
    local targetAnimal, currentBeam, currentNameTag = nil, nil, nil
    local AutoSwapDelay, AutoLockDistance, lastSwapTime = 1, 7, 0

    local player = game.Players.LocalPlayer
    local RunService = game:GetService("RunService")
    local character = player.Character or player.CharacterAdded:Wait()
    local root = character:WaitForChild("HumanoidRootPart")

    local rainbowColors = {
        Color3.fromRGB(255,0,0), Color3.fromRGB(255,127,0), Color3.fromRGB(255,255,0),
        Color3.fromRGB(0,255,0), Color3.fromRGB(0,0,255), Color3.fromRGB(75,0,130),
        Color3.fromRGB(148,0,211)
    }

    local function ESPNearestAnimal(target)
        if not target then return end
        local attachment0 = Instance.new("Attachment", root)
        local attachment1 = Instance.new("Attachment", target:FindFirstChild("HumanoidRootPart") or target.PrimaryPart)
        local beam = Instance.new("Beam")
        beam.Attachment0 = attachment0
        beam.Attachment1 = attachment1
        beam.Width0, beam.Width1 = 0.15, 0.15
        beam.FaceCamera = true
        beam.Segments = 10
        beam.Parent = workspace

        local billboard = Instance.new("BillboardGui")
        billboard.Name = "NameTag"
        billboard.Adornee = attachment1.Parent
        billboard.Size = UDim2.new(0,120,0,40)
        billboard.StudsOffset = Vector3.new(0,3,0)
        billboard.AlwaysOnTop = true

        local label = Instance.new("TextLabel", billboard)
        label.Size = UDim2.new(1,0,1,0)
        label.BackgroundTransparency = 1
        label.TextScaled = true
        label.Text = target.Name
        label.Font = Enum.Font.SourceSansBold
        label.TextStrokeTransparency = 0
        label.TextStrokeColor3 = Color3.new(0,0,0)
        billboard.Parent = target

        local step = 0
        RunService.RenderStepped:Connect(function(delta)
            step += delta or 0.016
            local t = (step * 5) % #rainbowColors + 1
            local i1 = math.floor(t) % #rainbowColors + 1
            local i2 = (i1 % #rainbowColors) + 1
            local alpha = t - math.floor(t)
            local color = rainbowColors[i1]:Lerp(rainbowColors[i2], alpha)
            beam.Color = ColorSequence.new(color)
            label.TextColor3 = color
        end)
        return beam, billboard
    end

    local function AutoLockNearestAnimal()
        local animals = workspace:WaitForChild("Animals"):WaitForChild("ClientModels"):GetChildren()
        if #animals < 2 then return nil end
        local closest, shortest = nil, math.huge
        for i = 2, #animals do
            local a = animals[i]
            local hrp = a:FindFirstChild("HumanoidRootPart") or a.PrimaryPart
            if hrp then
                local dist = (root.Position - hrp.Position).Magnitude
                if dist > AutoLockDistance and dist < shortest then
                    shortest = dist
                    closest = a
                end
            end
        end
        return closest
    end

    local function AutoSwapAnimal(target)
        if target then
            game:GetService("ReplicatedStorage"):WaitForChild("Remote")
                :WaitForChild("Animals"):WaitForChild("PlayerSwapAnimal"):FireServer(target.Name)
        end
    end

    local Main = Window:Tab({Title = "Main", Icon = "star"})
    Main:Section({Title = "‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏Å"})
    Main:Toggle({Title = "ESP ‡∏™‡∏±‡∏ï‡∏ß‡πå‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î", Value = ESPEnabled, Callback = function(v) ESPEnabled = v end})
    Main:Toggle({Title = "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ ‡∏•‡πá‡∏≠‡∏Å‡∏™‡∏±‡∏ï‡∏ß‡πå", Value = AutoLockEnabled, Callback = function(v) AutoLockEnabled = v end})
    Main:Slider({Title = "‡∏£‡∏∞‡∏¢‡∏∞ AutoLock", Min=1, Max=50, Value=AutoLockDistance, Callback=function(v) AutoLockDistance=v end})
    Main:Toggle({Title="‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏™‡∏•‡∏±‡∏ö‡∏™‡∏±‡∏ï‡∏ß‡πå", Value=AutoSwapEnabled, Callback=function(v) AutoSwapEnabled=v end})
    Main:Slider({Title="‡∏î‡∏µ‡πÄ‡∏•‡∏¢‡πå AutoSwap", Min=0.1, Max=5, Value=AutoSwapDelay, Callback=function(v) AutoSwapDelay=v end})

    RunService.RenderStepped:Connect(function(delta)
        if AutoLockEnabled then
            local newTarget = AutoLockNearestAnimal()
            if newTarget ~= targetAnimal then
                if currentBeam then currentBeam:Destroy() currentBeam=nil end
                if currentNameTag then currentNameTag:Destroy() currentNameTag=nil end
                targetAnimal = newTarget
                if targetAnimal and ESPEnabled then
                    currentBeam, currentNameTag = ESPNearestAnimal(targetAnimal)
                end
            end
        end
        if AutoSwapEnabled and targetAnimal then
            lastSwapTime += delta or 0.016
            if lastSwapTime >= AutoSwapDelay then
                AutoSwapAnimal(targetAnimal)
                lastSwapTime = 0
            end
        else
            lastSwapTime = 0
        end
    end)

    Window:Notify({Title="BOOM HUB", Desc="‡∏£‡∏∞‡∏ö‡∏ö‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô!", Time=4})

    -- ‚úÖ ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏£‡∏±‡∏ô‡πÄ‡∏™‡∏£‡πá‡∏à ‡πÇ‡∏´‡∏•‡∏î Mobile Keyboard (‡∏£‡∏ß‡∏°‡πÉ‡∏ô‡∏ô‡∏µ‡πâ)
    local Player = game.Players.LocalPlayer
    local VirtualInput = game:GetService("VirtualInputManager")
    local UIS = game:GetService("UserInputService")

    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "MobileKeyboardUI"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.Parent = Player:WaitForChild("PlayerGui")

    local dragging = false
    local dragButton = nil
    local lockPosition = false

    local function createButton(name, text, position, key)
        local button = Instance.new("TextButton")
        button.Name = name
        button.Size = UDim2.new(0, 70, 0, 70)
        button.Position = position
        button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        button.BackgroundTransparency = 0.4
        button.TextColor3 = Color3.new(1, 1, 1)
        button.TextScaled = true
        button.Font = Enum.Font.GothamBold
        button.Text = text
        button.BorderSizePixel = 0
        button.ZIndex = 10
        button.Parent = ScreenGui

        button.MouseButton1Down:Connect(function()
            VirtualInput:SendKeyEvent(true, Enum.KeyCode[key], false, game)
        end)

        button.MouseButton1Up:Connect(function()
            VirtualInput:SendKeyEvent(false, Enum.KeyCode[key], false, game)
        end)

        button.InputBegan:Connect(function(input)
            if lockPosition then return end
            if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
                dragButton = button
                button:SetAttribute("StartPos", input.Position)
                button:SetAttribute("StartButtonPos", button.Position)
            end
        end)

        UIS.InputChanged:Connect(function(input)
            if dragging and dragButton == button and (input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseMovement) then
                local delta = input.Position - button:GetAttribute("StartPos")
                button.Position = UDim2.new(
                    button:GetAttribute("StartButtonPos").X.Scale,
                    button:GetAttribute("StartButtonPos").X.Offset + delta.X,
                    button:GetAttribute("StartButtonPos").Y.Scale,
                    button:GetAttribute("StartButtonPos").Y.Offset + delta.Y
                )
            end
        end)

        UIS.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = false
                dragButton = nil
            end
        end)
    end

    createButton("LeftButton", "‚óÄÔ∏è", UDim2.new(0.08, 0, 0.78, 0), "A")
    createButton("RightButton", "‚ñ∂Ô∏è", UDim2.new(0.80, 0, 0.78, 0), "D")
    createButton("JumpButton", "SP", UDim2.new(0.88, 0, 0.70, 0), "Space")

    local LockButton = Instance.new("TextButton")
    LockButton.Size = UDim2.new(0, 70, 0, 50)
    LockButton.Position = UDim2.new(0.02, 0, 0.4, 0)
    LockButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    LockButton.BackgroundTransparency = 0.4
    LockButton.TextColor3 = Color3.new(1, 1, 1)
    LockButton.Text = "üîí ‡∏•‡πá‡∏≠‡∏Ñ‡∏õ‡∏∏‡πà‡∏°"
    LockButton.Font = Enum.Font.GothamBold
    LockButton.TextScaled = true
    LockButton.BorderSizePixel = 0
    LockButton.ZIndex = 10
    LockButton.Parent = ScreenGui

    LockButton.MouseButton1Click:Connect(function()
        lockPosition = not lockPosition
        if lockPosition then
            LockButton.Text = "üîì ‡∏õ‡∏•‡∏î‡∏•‡πá‡∏≠‡∏Ñ"
            LockButton.BackgroundColor3 = Color3.fromRGB(0,150,0)
        else
            LockButton.Text = "üîí ‡∏•‡πá‡∏≠‡∏Ñ‡∏õ‡∏∏‡πà‡∏°"
            LockButton.BackgroundColor3 = Color3.fromRGB(0,0,0)
        end
    end)

    local lockDragging = false
    local startPos, startButtonPos

    LockButton.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
            lockDragging = true
            startPos = input.Position
            startButtonPos = LockButton.Position
        end
    end)

    UIS.InputChanged:Connect(function(input)
        if lockDragging and (input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseMovement) then
            local delta = input.Position - startPos
            LockButton.Position = UDim2.new(
                startButtonPos.X.Scale,
                startButtonPos.X.Offset + delta.X,
                startButtonPos.Y.Scale,
                startButtonPos.Y.Offset + delta.Y
            )
        end
    end)

    UIS.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
            lockDragging = false
        end
    end)
end

-- ======= UI ‡∏£‡∏∞‡∏ö‡∏ö‡∏Ñ‡∏µ‡∏¢‡πå =======
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
ScreenGui.Name = "KeyUI_BoomHub"

local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0,360,0,220)
Frame.Position = UDim2.new(0.5,-180,0.5,-110)
Frame.BackgroundColor3 = Color3.fromRGB(10,10,10)
Frame.Active = true
Frame.Draggable = true
Instance.new("UICorner", Frame).CornerRadius = UDim.new(0,12)

local Title = Instance.new("TextLabel", Frame)
Title.AnchorPoint=Vector2.new(0.5,0)
Title.Position=UDim2.new(0.5,0,0,10)
Title.Size=UDim2.new(0.9,0,0,60)
Title.Text="Key"
Title.Font=Enum.Font.SourceSansBold
Title.TextSize=50
Title.TextColor3=Color3.new(1,1,1)
Title.BackgroundTransparency=1

local KeyBox = Instance.new("TextBox", Frame)
KeyBox.AnchorPoint=Vector2.new(0.5,0)
KeyBox.Size=UDim2.new(0,300,0,40)
KeyBox.Position=UDim2.new(0.5,0,0,105)
KeyBox.PlaceholderText="‡∏Å‡∏£‡∏≠‡∏Å‡∏Ñ‡∏µ‡∏¢‡πå‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà..."
KeyBox.Font=Enum.Font.SourceSansBold
KeyBox.TextSize=18
KeyBox.TextColor3=Color3.new(1,1,1)
KeyBox.BackgroundColor3=Color3.fromRGB(25,25,25)
KeyBox.BorderSizePixel=0
Instance.new("UICorner", KeyBox).CornerRadius = UDim.new(0,8)

local ConfirmButton = Instance.new("TextButton", Frame)
ConfirmButton.Size=UDim2.new(0,160,0,38)
ConfirmButton.Position=UDim2.new(0.5,-170,0,155)
ConfirmButton.Text="‡∏Å‡∏î‡∏¢‡∏∑‡∏ô‡∏¢‡∏±‡∏ô‡∏Ñ‡∏µ‡∏¢‡πå"
ConfirmButton.Font=Enum.Font.SourceSansBold
ConfirmButton.TextSize=16
ConfirmButton.BackgroundColor3=Color3.fromRGB(255,200,50)
ConfirmButton.TextColor3=Color3.new(0,0,0)
Instance.new("UICorner", ConfirmButton).CornerRadius=UDim.new(0,8)

local CopyButton = Instance.new("TextButton", Frame)
CopyButton.Size=UDim2.new(0,160,0,38)
CopyButton.Position=UDim2.new(0.5,10,0,155)
CopyButton.Text="‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏•‡∏¥‡πâ‡∏á‡∏î‡∏¥‡∏™"
CopyButton.Font=Enum.Font.SourceSansBold
CopyButton.TextSize=16
CopyButton.BackgroundColor3=Color3.new(1,1,1)
CopyButton.TextColor3=Color3.new(0,0,0)
Instance.new("UICorner", CopyButton).CornerRadius=UDim.new(0,8)

local Status = Instance.new("TextLabel", Frame)
Status.AnchorPoint=Vector2.new(0.5,0)
Status.Size=UDim2.new(0.9,0,0,22)
Status.Position=UDim2.new(0.5,0,0,200)
Status.BackgroundTransparency=1
Status.Text=""
Status.TextColor3=Color3.new(1,1,1)
Status.TextSize=16
Status.Font=Enum.Font.SourceSansBold

local function flashText(text,color)
	Status.Text=text
	Status.TextColor3=color or Color3.new(1,1,1)
	task.delay(1.5,function() Status.Text="" end)
end

CopyButton.MouseButton1Click:Connect(function()
	setclipboard(discordLink)
	flashText("‚úÖ ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏•‡∏¥‡πâ‡∏á‡πÅ‡∏•‡πâ‡∏ß!", Color3.fromRGB(255,255,120))
end)

ConfirmButton.MouseButton1Click:Connect(function()
	local keyInput = KeyBox.Text
	if keyInput == "" then
		flashText("‚ö†Ô∏è ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏Å‡∏£‡∏≠‡∏Å‡∏Ñ‡∏µ‡∏¢‡πå‡∏Å‡πà‡∏≠‡∏ô!", Color3.fromRGB(255,120,120))
	elseif keyInput:lower() == realKey:lower() then
		flashText("‚úÖ ‡∏Ñ‡∏µ‡∏¢‡πå‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!", Color3.fromRGB(100,255,100))
		for i=1,20 do Frame.Size = Frame.Size - UDim2.new(0,0,0,10) wait(0.02) end
		wait(0.2)
		Frame:Destroy()
		RunMainScript()
	else
		flashText("‚ùå ‡∏Ñ‡∏µ‡∏¢‡πå‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!", Color3.fromRGB(255,80,80))
	end
end)
